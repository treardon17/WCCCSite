div(class="contact-form parallax-container")
  h1(class="title-heading") Contact
  div(class="contact-form-container parallax-element")
    div(class="contact-form-info")
      div
        h2(class="sub-heading") Get in touch
        p Main Number:  509.326.9540
        p Fax:          509.326.2347
        p Address:      1603 N. Belt Street, Spokane, Washington 99205
    form(class="contact-form-form" action="contact-form.php")
      div
        p First name:
        input(class="contact-form-text-box" type="text" name="firstname")
        p Last name:
        input(class="contact-form-text-box" type="text" name="lastname")
        P Email:
        input(class="contact-form-text-box" type="text" name="email")
        p Phone:
        input(class="contact-form-text-box" type="text" name="phone")
        p Department:
          select(name="Department")
            option(value="Accounting") Accounting
            option(value="General Information") General Information
            option(value="Room Rental") Room Rental
            option(value="Executive Director") Executive Director
            option(value="Human Resources") Human Resources
            option(value="WIC") WIC
            option(value="Youth Development/Recreation") Youth Development/Recreation
            option(value="Learning Skills/Community Access") Learning Skills/Community Acess
        p Message:
        input(class="contact-form-message" type="text" name="Message")
        input(type="Submit" value="Submit" class="submit-button")
